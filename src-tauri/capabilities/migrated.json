{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": ["main"],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "fs:default",
    "dialog:default",
    {
      "identifier": "fs:allow-exists",
      "allow": [
        {
          "path": "*"
        }
      ]
    },
    {
      "identifier": "fs:allow-write",
      "allow": [
        {
          "path": "*"
        }
      ]
    },
    {
      "identifier": "fs:allow-read",
      "allow": [
        {
          "path": "*"
        }
      ]
    },
    {
      "identifier": "dialog:allow-open",
      "allow": [
        {
          "path": "*"
        }
      ]
    }
  ]
}
